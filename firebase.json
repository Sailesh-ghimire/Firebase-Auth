{
  "hosting": {
    "public": "out",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/login",
        "destination": "/index.html"
      },
      {
        "source": "/signup",
        "destination": "/index.html"
      }
    ],
    "error": {
      "404": "/404.html" 
    }
    
  }
}
